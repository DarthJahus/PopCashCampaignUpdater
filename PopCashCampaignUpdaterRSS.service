[Unit]
Description=PopCashCampaignUpdater
After=syslog.target
After=network.target

[Service]
Type=simple
ExecStart=/usr/bin/python3 /bots/PopCashCampaignUpdater/PopCashCampaignUpdaterRSS.py
WorkingDirectory=/bots/PopCashCampaignUpdater/
Restart=always
RestartSec=600

[Install]
WantedBy=multi-user.target
